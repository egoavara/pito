{
  "name": "pito",
  "version": "0.3.0",
  "description": "typescript based type define library",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamGreedy/pito.git"
  },
  "main": "./cjs/pito.js",
  "module": "./esm/pito.js",
  "exports": {
    ".": {
      "import": "./esm/pito.js",
      "require": "./cjs/pito.js"
    }
  },
  "files": [
    "cjs/**/*.d.ts",
    "cjs/**/*.js",
    "cjs/package.json",
    "esm/**/*.d.ts",
    "esm/**/*.js",
    "esm/package.json"
  ],
  "scripts": {
    "test": "tap --ts",
    "build": "npm run build:cjs & npm run build:esm",
    "build:cjs": "npm run clear:cjs && tsc -p ./cjs/tsconfig.json",
    "build:esm": "npm run clear:esm && tsc -p ./esm/tsconfig.json",
    "clear": "npm run clear:cjs & npm run clear:esm",
    "clear:cjs": "rimraf ./cjs/**/*.{js,js.map,d.ts,d.ts.map,tsbuildinfo}",
    "clear:esm": "rimraf ./esm/**/*.{js,js.map,d.ts,d.ts.map,tsbuildinfo}",
    "prepack": "npm run build"
  },
  "keywords": [
    "typescript",
    "json-schema"
  ],
  "author": "egoavara",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.33",
    "@types/tap": "^15.0.6",
    "ajv": "^8.11.0",
    "currency.js": "^2.0.4",
    "rimraf": "^3.0.2",
    "tap": "^16.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  }
}
